{
  "expo": {
    "name": "TIUE",
    "slug": "tiueapp",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "tiueapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.tiueapp",
      "icon": "./assets/images/icon.png",
      "splash": {
        "image": "./assets/images/icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIViewControllerBasedStatusBarAppearance": true,
        "UIStatusBarHidden": false
      }
    },
    "android": {
      "icon": "./assets/images/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/images/icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.tiueapp",
      "usesCleartextTraffic": true,
      "networkSecurityConfig": "./android/app/src/main/res/xml/network_security_config.xml"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-navigation-bar",
        {
          "position": "absolute",
          "visibility": "hidden"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "expo-navigation-bar": {
        "position": "absolute",
        "visibility": "hidden"
      },
      "eas": {
        "projectId": "62f609e2-aa10-4064-abbc-060003be393e"
      }
    }
  }
}
